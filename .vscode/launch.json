{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "NestJS: Start (npm run start:dev)",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "start:dev"],
        "port": 9229,
        "console": "integratedTerminal"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "NestJS: Launch Current File",
        "skipFiles": ["<node_internals>/**"],
        "program": "${file}",
        "console": "integratedTerminal"
      }
    ]
  }